spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver
spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true
spring.jpa.properties.hibernate.format_sql=true
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL57InnoDBDialect

spring.datasource.url=jdbc:mysql://host:port/db
spring.datasource.username=username
spring.datasource.password=password
spring.datasource.hikari.maximum-pool-size=10

custom.authentication.oauth.client_id=clientId
custom.authentication.oauth.client_secret=clientSecret
custom.authentication.jwt_signing_mac_key=jwtSigningKey
custom.authentication.access_token_validity_seconds=1000
custom.authentication.oauth.refresh_token_validity_seconds=2000
